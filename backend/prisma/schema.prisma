generator client {
    provider = "prisma-client-js"
    // output   = "../src/generated/prisma"
}

datasource db {
    provider = "mongodb"
    url      = env("DATABASE_URL")
}

model Customer {
    id        String @id @default(auto()) @map("_id") @db.ObjectId
    marca     String
    modelo    String
    imagem String
    descricao String
    anoKm  String
    local String
    preco Float
    created_at DateTime? @default(now())
    updated_at DateTime? @default(now())

    @@map("customers")
}

