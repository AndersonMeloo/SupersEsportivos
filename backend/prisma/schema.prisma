generator client {
    provider = "prisma-client-js"
    output   = "../src/generated/prisma"
}

datasource db {
    provider = "mongodb"
    url      = env("DATABASE_URL")
}

model Customer {
    id        String @id @default(auto()) @map("_id") @db.ObjectId
    marca     String
    modelo    String
    descricao String
    anoKm  String
    local String
    preco Float
}

